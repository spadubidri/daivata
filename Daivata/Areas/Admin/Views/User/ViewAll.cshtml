@model List<Daivata.Entities.AccountSummary>

@if (Model != null && Model.Count > 0)
{
    <div class="tab-content col-md-7">
        <table border="1" class="table table-bordered table-condensed table-striped">
            <thead>
                <tr>
                    <td class="col-md-2">Profile Id</td>
                    <td class="col-md-2">First Name</td>
                    <td class="col-md-2">Last Name</td>
                    <td class="col-md-2">Email</td>
                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var usr in Model)
                    {
                        <tr>
                            <td>
                                <div>@usr.ProfileID</div>
                            </td>
                            <td><div>@usr.FirstName</div></td>
                            <td><div>@usr.LastName</div></td>
                            <td><div>@usr.Email</div></td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
}